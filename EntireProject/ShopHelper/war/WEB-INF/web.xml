<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>ShopHelperServlet</servlet-name>
		<servlet-class>sh.ShopHelperServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShopHelperServlet</servlet-name>
		<url-pattern>/ShopHelperServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoadStartingData</servlet-name>
		<servlet-class>sh.LoadStartingData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoadStartingData</servlet-name>
		<url-pattern>/LoadStartingData</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReportEntryServlet</servlet-name>
		<servlet-class>sh.ReportEntryServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ReportEntryServlet</servlet-name>
		<url-pattern>/ReportEntryServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReportEntryStoreServlet</servlet-name>
		<servlet-class>sh.ReportEntryStoreServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ReportEntryStoreServlet</servlet-name>
		<url-pattern>/ReportEntryStoreServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RemoveListServlet</servlet-name>
		<servlet-class>sh.RemoveListServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RemoveListServlet</servlet-name>
		<url-pattern>/RemoveListServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>HomePage.jsp</welcome-file>
	</welcome-file-list>
</web-app>
